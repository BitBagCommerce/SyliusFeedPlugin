<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="setono_sylius_feed.resolver.feed_extension" class="Setono\SyliusFeedPlugin\Resolver\FeedExtensionResolver">
            <argument type="service" id="setono_sylius_feed.registry.feed_type"/>
            <argument type="service" id="twig"/>
        </service>

        <service id="setono_sylius_feed.resolver.feed_path" class="Setono\SyliusFeedPlugin\Resolver\FeedPathResolver">
            <argument type="service" id="setono_sylius_feed.resolver.feed_extension"/>
        </service>
    </services>
</container>
